<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="document_type_e_ticket" model="document.type">
            <field name="name">E-ticket</field>
            <field name="code">ET</field>
            <field name="description">Documento electrónico tipo ticket</field>
            <field name="active">True</field>
        </record>

        <record id="document_type_e_factura" model="document.type">
